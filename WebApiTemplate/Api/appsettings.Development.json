{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "AppSettings": {
    "ProjectInfo": {
      "Name": "Insert_Project_Name",
      "Description": "Insert_Project_Description",
      "Version": "1.0.0",
      "Company": "Insert_Company_Name",
      "Product": "Insert_Product_Name",
      "Contact": {
        "Name": "infoteam Hellas MIKE",
        "Email": "info@infoteam-software.gr",
        "Url": "https://infoteam-software.gr"
      }
    }
  }
}
